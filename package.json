{
  "name": "smart-health-consulting-system",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "set NODE_OPTIONS=--require ./expo-builtin-filter.js --require ./expo-fs-mkdir-hook.js && set EXPO_NO_METRO_EXTERNALS=1 && expo start",
    "start:raw": "expo start",
    "start:hack": "node ./expo-node24-hack.js",
    "android": "set NODE_OPTIONS=--require ./expo-builtin-filter.js --require ./expo-fs-mkdir-hook.js && set EXPO_NO_METRO_EXTERNALS=1 && expo start --android",
    "android:raw": "expo start --android",
    "android:hack": "node ./expo-node24-hack.js --android",
    "tunnel": "set NODE_OPTIONS=--require ./expo-builtin-filter.js --require ./expo-fs-mkdir-hook.js && set EXPO_NO_METRO_EXTERNALS=1 && expo start --tunnel",
    "tunnel:hack": "node ./expo-node24-hack.js --tunnel",
    "ios": "expo start --ios",
    "web": "expo start --web"
  },
  "dependencies": {
    "@clerk/clerk-expo": "^1.0.0",
    "@expo/metro-runtime": "~3.1.3",
    "@react-navigation/bottom-tabs": "^6.5.20",
    "@react-navigation/native": "^6.1.17",
    "@react-navigation/stack": "^6.3.29",
    "@types/react-dom": "^18.2.25",
    "axios": "^1.6.8",
    "expo": "~50.0.14",
    "expo-application": "^5.8.4",
    "expo-auth-session": "^5.4.0",
    "expo-font": "~11.10.3",
    "expo-secure-store": "~12.8.1",
    "expo-status-bar": "~1.11.1",
    "moment": "^2.30.1",
    "react": "18.2.0",
    "react-dom": "^18.2.0",
    "react-native": "0.73.6",
    "react-native-gesture-handler": "~2.14.0",
    "react-native-loading-spinner-overlay": "^3.0.1",
    "react-native-safe-area-context": "4.8.2",
    "react-native-screens": "~3.29.0",
    "react-native-web": "^0.19.11",
    "typescript": "^5.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0"
  },
  "engines": {
    "node": ">=18 <21"
  },
  "private": true
